 #include <bits/stdc++.h>
using namespace std;
int main() {
	ios_base::sync_with_stdio(false);
        cin.tie(NULL);
    int n;
    cin>>n;
    int a[n-1],i=0;
    for(i=0;i<n-1;i++)
         cin>>a[i];
    sort(a,a+n-1);
  for(i=0;i<n-1;i++)
        if(i!=a[i]-1)
         {           
            cout<<(i+1)<<'\n';     
             break;         
         }
        if(i==n-1)
          cout<<(n)<<'\n';
    return 0;
}
